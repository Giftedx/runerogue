{
  "name": "@runerogue/server",
  "version": "0.1.0",
  "description": "RuneRogue multiplayer server with ECS and OSRS mechanics",
  "main": "dist/server/index.js",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "build:clean": "rimraf dist && npm run build",
    "start": "node dist/src/server/index.js",
    "manage-rules": "ts-node scripts/manage_rules.ts",
    "dev": "npm run extract-assets && npm run build && node dist/src/server/index.js",
    "extract-assets": "tsx src/assets/extract-assets-script.ts",
    "extract-assets:ui": "ts-node src/assets/extract-assets-script.ts --type ui",
    "extract-assets:player": "ts-node src/assets/extract-assets-script.ts --type player",
    "extract-assets:npc": "ts-node src/assets/extract-assets-script.ts --type npc",
    "verify-assets": "tsx src/assets/verify-assets.ts",
    "test-osrs-integration": "ts-node src/assets/test-osrs-integration.ts",
    "test-pipeline": "tsx src/assets/test-pipeline.ts",
    "test-mcp-discovery": "tsx src/assets/test-mcp-discovery.ts",
    "run-mcp-extraction": "tsx src/assets/practical-mcp-extraction.ts",
    "comprehensive-extract": "tsx src/assets/comprehensive-osrs-extractor.ts",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{ts,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,json,md}\"",
    "type-check": "tsc --noEmit --project tsconfig.json",
    "type-check:tests": "tsc --noEmit --project tsconfig.test.json",
    "test": "cross-env COLYSEUS_URI=ws://localhost:2567 jest --ci --runInBand --config jest.config.js",
    "test:watch": "jest --watch --config jest.config.js",
    "test:coverage": "jest --coverage --config jest.config.js",
    "test:ci": "cross-env COLYSEUS_URI=ws://localhost:2567 jest --ci --runInBand --config jest.config.js",
    "extract-cache": "tsx src/assets/osrs-cache-reader.ts",
    "extract-cache-fallback": "tsx src/assets/osrs-cache-reader-fallback.ts",
    "coordinate-extraction": "tsx src/assets/comprehensive-asset-coordinator.ts",
    "install-cache-reader": "npm install osrscachereader",
    "plan-pipeline": "tsx src/assets/asset-pipeline-implementation-planner.ts"
  },
  "keywords": [
    "runescape",
    "discord",
    "game",
    "colyseus",
    "typescript"
  ],
  "dependencies": {
    "@runerogue/shared": "workspace:*",
    "@runerogue/osrs-data": "workspace:*",
    "@colyseus/core": "^0.16.18",
    "@colyseus/monitor": "^0.16.0",
    "@colyseus/schema": "^3.0.39",
    "@colyseus/tools": "^0.16.9",
    "@colyseus/ws-transport": "^0.16.5",
    "@discord/embedded-app-sdk": "^2.0.0",
    "@discordjs/rest": "^2.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/pixi.js": "^4.8.9",
    "@types/rot-js": "^0.6.5",
    "@types/winston": "^2.4.4",
    "axios": "^1.9.0",
    "axios-retry": "^4.5.0",
    "bitecs": "^0.3.40",
    "colyseus": "^0.16.0",
    "commander": "^10.0.0",
    "cors": "^2.8.5",
    "discord-api-types": "^0.37.81",
    "discord.js": "^14.15.3",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "express-rate-limit": "^7.1.5",
    "fs-extra": "^11.3.0",
    "graceful-fs": "^4.2.11",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "node-cache": "^5.1.2",
    "node-fetch": "^2.7.0",
    "oldschooljs": "^2.6.1",
    "osrscachereader": "^1.1.0",
    "p-limit": "^6.2.0",
    "phaser": "^3.90.0",
    "pixi.js": "^8.10.1",
    "reflect-metadata": "^0.2.2",
    "rot-js": "^2.2.1",
    "simplex-noise": "^4.0.3",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "ws": "^8.18.2",
    "yamljs": "^0.3.0",
    "zod": "^3.25.51"
  },
  "devDependencies": {
    "@colyseus/testing": "^0.16.3",
    "@types/cors": "^2.8.17",
    "@types/express": "5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/node": "^24.0.3",
    "@types/node-fetch": "^2.6.12",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "axios-mock-adapter": "^2.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^30.0.2",
    "jest-fetch-mock": "^3.0.3",
    "jest-junit": "^16.0.0",
    "mock-socket": "^9.3.1",
    "prettier": "^3.2.5",
    "rimraf": "^6.0.1",
    "sharp": "^0.34.2",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.1",
    "typescript": "^5.3.3"
  }
}
