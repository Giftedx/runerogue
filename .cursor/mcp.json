{
  "mcpServers": {
    "github": {
      "command": "uvx",
      "args": ["mcp-server-git"]
    },
    "sequential-thinking": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"]
    },
    "osrs": {
      "command": "node",
      "args": ["dist/mcp-server.js"],
      "cwd": "server-ts"
    },
    "memory": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-memory"]
    },
    "tavily": {
      "command": "npx",
      "args": ["-y", "@mcptools/mcp-tavily"],
      "env": {
        "TAVILY_API_KEY": "${TAVILY_API_KEY}"
      }
    },
    "mcpso": {
      "command": "node",
      "args": ["server.js"],
      "cwd": "mcp-repos/mcpso"
    },
    "open-mcp-client": {
      "command": "node",
      "args": ["dist/index.js"],
      "cwd": "mcp-repos/open-mcp-client"
    },
    "magic-mcp": {
      "command": "node",
      "args": ["dist/index.js"],
      "cwd": "mcp-repos/magic-mcp"
    },
    "runerogue-game": {
      "command": "node",
      "args": ["dist/mcp-server.js"],
      "cwd": "server-ts"
    },
    "browser-tools": {
      "command": "npx",
      "args": ["@agentdeskai/browser-tools-mcp@latest"]
    }
  }
}
