{
  "name": "runerogue",
  "version": "0.1.0",
  "description": "RuneScape Discord Game",
  "main": "dist/server/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server/index.js",
    "manage-rules": "ts-node scripts/manage_rules.ts",
    "dev": "npm run build && node dist/server/index.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.{ts,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,json,md}\"",
    "type-check": "tsc --noEmit",
    "test": "cross-env COLYSEUS_URI=ws://localhost:2567 jest --ci --runInBand",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "cross-env COLYSEUS_URI=ws://localhost:2567 jest --ci --runInBand"
  },
  "keywords": [
    "runescape",
    "discord",
    "game",
    "colyseus",
    "typescript"
  ],
  "dependencies": {
    "@colyseus/core": "^0.16.18",
    "@colyseus/monitor": "^0.16.0",
    "@colyseus/schema": "^3.0.39",
    "@colyseus/ws-transport": "^0.16.0",
    "@discordjs/rest": "^2.0.0",
    "@types/rot-js": "^0.6.5",
    "@types/winston": "^2.4.4",
    "axios-retry": "^4.5.0",
    "colyseus": "^0.16.0",
    "commander": "^10.0.0",
    "cors": "^2.8.5",
    "discord-api-types": "^0.37.81",
    "discord.js": "^14.15.3",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "node-cache": "^5.1.2",
    "node-fetch": "^2.7.0",
    "reflect-metadata": "^0.2.2",
    "rot-js": "^2.2.1",
    "simplex-noise": "^4.0.3",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "ws": "^8.18.2",
    "yamljs": "^0.3.0",
    "zod": "^3.25.51"
  },
  "devDependencies": {
    "@colyseus/testing": "^0.16.0",
    "@types/cors": "^2.8.17",
    "@types/express": "5.0.2",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/node": "^22.15.29",
    "@types/node-fetch": "^2.6.12",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "axios-mock-adapter": "^2.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-junit": "^16.0.0",
    "mock-socket": "^9.3.1",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
